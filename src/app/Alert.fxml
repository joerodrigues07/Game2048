<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="parent" prefHeight="410.0" prefWidth="410.0" styleClass="main-pane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.AlertController">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
   <children>
      <Label fx:id="message" alignment="CENTER" layoutX="34.0" layoutY="114.0" prefHeight="77.0" prefWidth="343.0" text="Game Over!" textAlignment="CENTER" textFill="#573e2a">
         <font>
            <Font name="Calibri Bold" size="53.0" />
         </font>
      </Label>
      <Button fx:id="btnTryAgain" alignment="CENTER" layoutX="135.0" layoutY="205.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="141.0" styleClass="btn-close" stylesheets="@style.css" text="Try again">
         <font>
            <Font name="Calibri" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
